{
  "expo": {
    "name": "Poopipado",
    "slug": "Poopipado",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#E06464"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.poopipado.poopipado",
      "supportsTablet": true
    },
    "android": {
      "package": "com.poopipado.poopipado",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3D3F62"
      }
    }
  }
}
